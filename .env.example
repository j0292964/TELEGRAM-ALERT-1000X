TELEGRAM_TOKEN=TELEGRAM_TOKEN
TELEGRAM_CHAT_ID=<your_channel_or_group_id>  # use @userinfobot to find it
# Comma separated list of whale addresses to monitor
WATCHED_WALLETS=0xWallet1,0xWallet2
# Etherscan API key for querying wallet transactions
ETHERSCAN_API_KEY=<etherscan_key>
# QuickNode RPC endpoint (optional, used instead of Etherscan)
QUICKNODE_RPC_URL=https://your-quicknode-endpoint
# Generic Web3 HTTP provider (optional)
WEB3_PROVIDER_URL=https://your-rpc-endpoint
# Endpoint that returns profitable whale wallets (optional)
WHALE_FINDER_URL=https://example.com/whales
WHALE_FINDER_API_KEY=<api_key>
# Local file storing discovered whales
WHALE_DB_PATH=whales.json
# Token for QuickNode Storage add-on (optional)
QUICKNODE_STORAGE_TOKEN=<your_token>
# BitHub storage endpoint (optional)
BITHUB_API_URL=
BITHUB_API_TOKEN=
